<?xml version="1.0" encoding="UTF-8" ?>
<!-- d:接口方法名 name:需要验证的属性名 
      maxLength:最大长度,如果最大长度小于1就不验证；
      isNull:是否可以为空，1:是，0：否 
	 type:属性名的类型，支持String，int，long,json,jsonArr
	 isValid:tokenId是否有效性验证，1：验证有效性，0：不需要验证有效性 
	 regular:正则表达式，如：特殊字符，枚举类型的正则表达式("^[1-9][0-9]?$") 后期可以扩展统一验证日期格式等。 -->
<body>

	<msg method="/get_upload_token">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="32"/>
		<attribute name="type" maxLength="1" isNull="0" type="String"  minLength="1"/>
		<attribute name="cover_upload" maxLength="1" isNull="0" type="String"  minLength="0"/>
		<attribute name="file_name" maxLength="512" isNull="1" type="String"  minLength="0"/>
	</msg>
	<msg method="/get_download_token">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="32"/>
		<attribute name="url" maxLength="512" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<!-- 用户 -->
	<msg method="/user/check_login_id">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="32"/>
	</msg>
	
	<msg method="/code/get">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="11"/>
		<attribute name="type" maxLength="1" isNull="0" type="String"  minLength="1"/>
	</msg>
	
	<msg method="/code/check">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="11"/>
		<attribute name="code" maxLength="6" isNull="0" type="String"  minLength="6"/>
	</msg>
	
	<msg method="/account/reg">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="11"/>
		<attribute name="vali_code" maxLength="20" isNull="0" type="String"  minLength="32"/>
		<attribute name="passwd"  maxLength="200" isNull="0" type="String"  minLength="32"/>
		<attribute name="device_id"  maxLength="200" isNull="0" type="String"  minLength="32"/>
	</msg>
	<msg method="/user/set_identity">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="32"/>
		<attribute name="user_type" maxLength="1" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/account/login">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="0"/>
		<attribute name="passwd" maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="type" maxLength="1" isNull="0" type="String"  minLength="1"/>
	</msg>
	
	<msg method="/account/logout">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="32"/>
	</msg>
	<msg method="/account/one_key_login">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="type" maxLength="1" isNull="0" type="String"  minLength="1"/>
	</msg>
	<msg method="/account/third_login">
		<attribute name="open_id" maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="access_token" maxLength="128" isNull="0" type="String"  minLength="0"/>
		<attribute name="refresh_token" maxLength="128" isNull="1" type="String"  minLength="0"/>
		<attribute name="third_avatar_address" maxLength="512" isNull="0" type="String"  minLength="0"/>
		<attribute name="third_nick_name" maxLength="128" isNull="0" type="String"  minLength="0"/>
		<attribute name="type" maxLength="1" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/account/set_third_login">
		<attribute name="open_id" maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/user/get_user_list">
		<attribute name="user_ids"  maxLength="20000" isNull="0"  type="jsonArr"  minLength="0"/>
	</msg>
	<msg method="/user/edit_info">
		<attribute name="user_id"  maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="gender"  maxLength="1" isNull="1" type="String"  minLength="0"/>
		<attribute name="nick_name"  maxLength="20" isNull="1" type="String"  minLength="0"/>
	</msg>
	<msg method="/account/set_passwd">
		<attribute name="user_id"  maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="old_passwd"  maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="new_passwd"  maxLength="200" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/account/reset_passwd">
		<attribute name="login_id" maxLength="20" isNull="0" type="String"  minLength="11"/>
		<attribute name="new_passwd" maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="vali_code" maxLength="32" isNull="0" type="String"  minLength="32"/>
	</msg>
	<msg method="/user/set_avatar">
		<attribute name="user_id" maxLength="20" isNull="0" type="String"  minLength="0"/>
		<attribute name="avatar_address" maxLength="512" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/user/check_phone">
		<attribute name="phones" maxLength="200000" isNull="0" type="jsonArr"  minLength="0"/>
	</msg>
	
	<!-- 群组、社交 -->
	<msg method="/group/add">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_name" maxLength="200" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_desc" maxLength="200" isNull="1" type="String"  minLength="0"/>
		<attribute name="group_pic" maxLength="512" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/group/get">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/group/edit">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="0" type="int"  minLength="0"/>
		<attribute name="group_name" maxLength="200" isNull="1" type="String"  minLength="0"/>
		<attribute name="group_desc" maxLength="200" isNull="1" type="String"  minLength="0"/>
		<attribute name="group_pic" maxLength="512" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/group/set_pic">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="0" type="int"  minLength="0"/>
		<attribute name="group_pic" maxLength="512" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/group/del">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/member/query">
		<attribute name="group_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="record_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/member/add">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="0" type="int"  minLength="0"/>
		<attribute name="apply_desc" maxLength="200" isNull="1" type="String"  minLength="0"/>
		<attribute name="list" maxLength="20000" isNull="0" type="jsonArr"  minLength="0"/>
	</msg>
	
	<msg method="/member/edit">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="member_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_nick_name" maxLength="200" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/member/del">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="group_id" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="list" maxLength="2000" isNull="0" type="jsonArr"  minLength="0"/>
	</msg>
	
	<msg method="/member/invitation">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="group_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/group/get">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="apply_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="stat" maxLength="1" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/likes/click">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="like_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="type" maxLength="32" isNull="1" type="String"  minLength="0"/>
		<attribute name="opt" maxLength="32" isNull="1" type="int"  minLength="0"/>
	</msg>
	
	
	<msg method="/comment/publish">
		<attribute name="user_id"  maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="comm_obj_id"  maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="content"  maxLength="32" isNull="1" type="String"  minLength="0"/>
		<attribute name="type"  maxLength="1" isNull="1" type="String"  minLength="0"/>
		<attribute name="original_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/pub">
		<attribute name="/dynamic/pub" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="content" maxLength="200" isNull="0" type="int"  minLength="0"/>
		<attribute name="video_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
		<attribute name="access_auth" maxLength="32" isNull="1" type="String"  minLength="0"/>
		<attribute name="pic_list" maxLength="220000" isNull="1" type="jsonArr"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/get_group_list">
		<attribute name="group_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="record_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/get_user_group">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="record_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/del">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="dynamic_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/get_user_list">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="20" isNull="0" type="int"  minLength="0"/>
		<attribute name="record_id" maxLength="32" isNull="1" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/dynamic/detail">
		<attribute name="dynamic_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	
	<msg method="/member/handle">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/comment/query">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="original_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="size" maxLength="32" isNull="0" type="int"  minLength="0"/>
		<attribute name="record_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	<msg method="/comment/del">
		<attribute name="user_id" maxLength="32" isNull="0" type="String"  minLength="0"/>
		<attribute name="record_ids" maxLength="32" isNull="0" type="String"  minLength="0"/>
	</msg>
	
</body>